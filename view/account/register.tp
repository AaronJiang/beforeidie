{include file='uheader.tc' title='注册' page='page-register'}{include file='slogan.tc'}{literal}<script type='text/javascript'>$(document).ready(function(){	$("#form-register").validationEngine();})</script>{/literal}<div id='form-wap'>	<form id='form-register' class='clearfix' action='AccountC.php'>		<input type='text' title='你常用的邮箱' class='validate[required, custom[email]]' placeholder='邮箱' autocomplete='off' name='email' />		<input type='text' title='你的称呼，亲！' class='validate[required]' placeholder='称呼' autocomplete='off' name='username' />		<input type='password' title='设置密码，6-16位之间，建议数字和字母混合。' id='pwd' class='validate[required, minSize[6]]' placeholder='密码' autocomplete='off' name='password' />			<input type='password' title='重复一遍又不会怀孕！' class='validate[required, equals[pwd]]' placeholder='重复密码' autocomplete='off' name='re-password' />				<div class='form-footer'>			<a href='AccountC.php?act=p_login'>登陆</a>			<input type='submit' value='注册'>		</div>				<input type='hidden' name='act' value='register'>	</form></div>{include file='ufooter.tc'}