<div class='panel-header'>	<div class='panel-title'>{$title}</div>			<div class='panel-cmd-wapper'>			{if {$cmd|default:''} != '' && (({$isCreator|default:''} != '' && $isCreator) || {$isCreator|default:''} == '')}			<span class='panel-underline'>_ _ _</span>			<a class="panel-cmd"				{if {$cmdID|default:''} != ''}				id="{$cmdID|default:''}"				{/if}				{if {$link|default:''} != ''}				href="{$link}"				{/if}				>{$cmd}</a>			{/if}		</div></div>